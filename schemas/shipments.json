{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://shipit.com/order.schema.json",
    "title": "Order",
    "description": "order for shipment.",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "productId": {
            "type": "integer",
            "minimum": 1000
        },
        "addr": {
            "type": "string"
        },
        "zipcode": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "name",
        "productId",
        "addr",
        "zipcode"
    ]
}